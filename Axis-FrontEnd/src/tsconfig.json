{
  "compilerOptions": {
    "baseUrl": "",
    "paths": {
      "@login/*": ["app/login/*"],
      "@timesheet/*": ["app/timesheet/*"],
      "@services/*": ["app/services/*"]
    },
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types"
    ]
  }
}
